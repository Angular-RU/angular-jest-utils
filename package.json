    "devDependencies": {
        "@angular-ru/eslint-config": "12.5.0",
        "@angular-ru/prettier-config": "12.7.0",
        "@angular-ru/tsconfig": "12.1.0",
        "@angular/compiler": "8.2.14",
        "@angular/core": "8.2.14",
        "@types/node": "13.13.4",
        "eslint": "6.8.0",
        "npx": "10.2.2",
        "sort-package-json": "1.42.1",
        "ts-node": "8.9.1",
        "tslib": "1.11.1",
        "typescript": "3.8.3"
    }
