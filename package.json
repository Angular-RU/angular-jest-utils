        "test": "jest --config ./jest.config.js --colors",
        "test:ci": "jest --config ./jest.config.js --colors --coverage"
    },
    "prettier": "@angular-ru/prettier-config",
    "dependencies": {
        "@types/jest": "25.2.1",
        "jest": "25.5.3",
        "jest-preset-angular": "8.1.3",
