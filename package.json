    "license": "MIT",
    "author": "Maxim Ivanov",
    "main": "./lib/index.js",
    "typings": "./lib/index.d.ts",
    "scripts": {
        "build": "tsc",
        "format": "npx sort-package-json && npx prettier --write \"**/*.{ts,html,css,scss,md,js,json}\"",
        "lint": "npx eslint --fix **/*.ts",
